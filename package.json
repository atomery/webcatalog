{
  "name": "WebCatalog",
  "description": "Run Web Apps like Real Apps",
  "version": "22.0.0-alpha",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx patch-package && electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src ./dist.js --ext js",
    "lint:fix": "eslint ./public ./src ./dist.js --ext js --fix",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cross-env API_URL=http://localhost:3000/api electron .\" -k",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "dist-local": "yarn build && cross-env NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false node dist.js"
  },
  "repository": "https://github.com/atomery/webcatalog",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com"
  },
  "dependencies": {
    "axios": "0.19.2",
    "blueimp-md5": "2.16.0",
    "cheerio": "1.0.0-rc.3",
    "command-exists": "1.2.9",
    "decompress": "4.2.1",
    "electron-context-menu": "2.0.1",
    "electron-is-dev": "1.2.0",
    "electron-packager": "14.2.1",
    "electron-settings": "3.2.0",
    "electron-updater": "4.3.3",
    "electron-window-state": "5.0.3",
    "fast-xml-parser": "3.17.4",
    "follow-redirects": "1.11.0",
    "fs-extra": "9.0.1",
    "hasha": "5.2.0",
    "icon-gen": "2.0.0",
    "is-url": "1.2.4",
    "jimp": "0.13.0",
    "menubar": "8.0.2",
    "node-fetch": "2.6.0",
    "proxy-agent": "3.1.1",
    "read-chunk": "3.2.0",
    "regedit": "3.0.3",
    "semver": "7.3.2",
    "sudo-prompt": "9.2.1",
    "tmp": "0.2.1",
    "windows-shortcuts": "0.1.6",
    "yargs-parser": "18.1.3"
  },
  "devDependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "algoliasearch": "4.3.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "electron": "8.3.2",
    "electron-builder": "22.7.0",
    "electron-notarize": "0.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.1",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.75",
    "wait-on": "5.0.1"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
